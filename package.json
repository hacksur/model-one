{
  "name": "model-one",
  "description": "Model for D1",
  "version": "0.3.0",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/hacksur/model-one"
  },
  "author": "Julian Clatro <hola@julianclatro.com> (https://hacksur.com)",
  "contributors": [
    "Julian Clatro <hola@julianclatro.com> (https://hacksur.com)"
  ],
  "license": "MIT",
  "scripts": {
    "watch-build": "tsc -w",
    "watch-test": "ava --watch",
    "dev": "concurrently 'npm:watch-*'",
    "test": "  NODE_NO_WARNINGS=1 npx ava",
    "build": "tsc"
  },
  "keywords": [
    "d1",
    "model",
    "form"
  ],
  "devDependencies": {
    "ava": "6.3.0",
    "browser-env": "^3.3.0",
    "concurrently": "^7.3.0",
    "joi": "latest",
    "miniflare": "4.20250508.0",
    "better-sqlite3": "11.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">= 10"
  },
  "bugs": {
    "url": "https://github.com/hacksur/model-one/issues",
    "email": "jclatro@yahoo.com"
  },
  "homepage": "https://github.com/hacksur/model-one",
  "volta": {
    "node": "22.15.0"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "timeout": "2m",
    "workerThreads": false,
    "environmentVariables": {
      "NODE_ENV": "test",
      "TS_NODE_PROJECT": "test/tsconfig.json"
    }
  }
}